440\u0438\u043f\u0442, \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u0443\u0435\u043c\u044b\u0439 \u043f\u0440\u0438 \u0440\u0435\u0433\u0438\u0441\u0442\u0440\u0430\u0446\u0438\u0438: <script> import axios from 'axios'; export default { data() { return { username: '', password: '' } }, methods: { signup() { axios.post('http://127.0.0.1:8000/register_js/', { username: this.username, password: this.password }) .then(response => { console.log(response.data); if(response.status==200){this.$router.push({ path: '/login'});} }); }, gotoSignIn() { this.$router.push({ path: '/login'}); } } } </script> \u0421\u043a\u0440\u0438\u043f\u0442\u044b, \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u0443\u0435\u043c\u044b\u0435 \u0434\u043b\u044f \u043f\u043e\u043b\u0443\u0447\u0435\u043d\u0438\u044f \u0441\u043f\u0438\u0441\u043a\u0430 \u0433\u0440\u0443\u043f\u043f \u0438 \u0440\u0435\u0444\u0435\u0440\u0435\u043d\u0441\u0430 \u043a \u0441\u043f\u0438\u0441\u043a\u0443 \u043f\u0440\u0435\u043f\u043e\u0434\u0430\u0432\u0430\u0442\u0435\u043b\u0435\u0439: <script> import axios from 'axios'; export default { data() { return { items: [] }; }, created() { axios.get('http://127.0.0.1:8000/group_js/') .then(response => { this.items = response.data; }) .catch(error => { console.error(error); }); }, methods : { showGroup(item) { this.$router.push({ path: item}); }, gotoTutors() { this.$router.push({ path: '/tutor/'}); } } } </script> \u0420\u0435\u0437\u0443\u043b\u044c\u0442\u0430\u0442\u044b \u0427\u0442\u043e\u0431\u044b \u0441\u0434\u0435\u043b\u0430\u0442\u044c \u0438\u043d\u0442\u0435\u0440\u0444\u0435\u0439\u0441\u044b \u0431\u043e\u043b\u0435\u0435 \u043a\u0440\u0430\u0441\u0438\u0432\u044b\u043c\u0438 \u0431\u044b\u043b\u0438 \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u043d\u044b \u0433\u043e\u0442\u043e\u0432\u044b\u0435 \u0440\u0435\u0448\u0435\u043d\u0438\u044f \u0441 bootstrap:","title":"\u041b\u04204"},{"location":"index3/#4","text":"","title":"\u041b\u0430\u0431